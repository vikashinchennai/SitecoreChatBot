
@using Amgen.Web.Helper.SitecoreExtensions.Extensions;
@inherits Glass.Mapper.Sc.Web.Mvc.GlassView<Amgen.Web.Feature.ChatBot.Models.ChatStart>

@Model.DisplayMessage;
@Model.QuestionDisplay;


@foreach (var item in Model.AnswerOptions)
{
    @item.OptionDisplayText;
    @item.UserInputField;
    @item.GoBack;

    @item.CallerApi;
    @item.EndOfChat;
    @item.EndMessage;

}